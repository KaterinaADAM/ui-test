<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ECMS Functional Test" verbose="1" preserve-order="true">
	<test name="ecms-func-se-ckeditor" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.CKEditor_Formatting" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.CKEditor_InlineEditing" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.CKEditor_InsertObject" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.test01_ChangeSkinOfCKEditorToKama" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.ECMS_SE_CKeditor_In_Content_Add" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.ECMS_SE_CKEditor_PublishActivity" />
			<class name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.ckeditor.ECMS_SE_CKeditor_Usability" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>