<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ECMS Functional Test" verbose="1">
	<test name="ecms-func" enabled="true">
		<groups>
		  <run>
		    <exclude name="pending"/>
		  </run>
		</groups>
		<packages>
			<package name="org.exoplatform.selenium.platform.ecms.functional.dms.siteexplorer.admin" />
			<package name="org.exoplatform.selenium.platform.ecms.functional.dms.siteexplorer.collaboration" />
			<package name="org.exoplatform.selenium.platform.ecms.functional.dms.siteexplorer.info" />
			<package name="org.exoplatform.selenium.platform.ecms.functional.dms.siteexplorer.publication" />
		</packages>
	</test>

	<listeners>
    <listener class-name="org.exoplatform.selenium.TestReportListener"/>
  </listeners>
</suite>
