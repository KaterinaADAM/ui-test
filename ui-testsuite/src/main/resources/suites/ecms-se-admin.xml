<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ECMS Functional Test" verbose="1" preserve-order="true">
	<test name="ecms-func-se-admin" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_Export" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_Import_Folder" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_Import_IllustratedWebContent" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_Import_WebContent" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_ManageAction" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_ManageCategories" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_ManageRelation" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.admin.ECMS_SE_Admin_ManageVersions" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>