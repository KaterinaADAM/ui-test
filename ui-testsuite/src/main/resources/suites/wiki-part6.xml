<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Wiki Functional Test" verbose="1" preserve-order="true">
	<test name="wiki-func-permis-wikipermis" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.permissions.wikipermissions.Wiki_Permissions_WikiPermissions_Add" />
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.permissions.wikipermissions.Wiki_Permissions_WikiPermissions_CheckPermission" />
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.permissions.wikipermissions.Wiki_Permissions_WikiPermissions_Delete" />
		</classes>
	</test>
	<test name="wiki-func-basicaction" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.basicaction.Wiki_BasicAction_Intergration" />
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.basicaction.Wiki_BasicAction_Move_MoveControl" />
			<class
				name="org.exoplatform.selenium.platform.wiki.functional.basicaction.Wiki_BasicAction_Watch_Unwatch" />
		</classes>
	</test>

	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>
