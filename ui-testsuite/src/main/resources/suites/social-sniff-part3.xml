<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-part2" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.social.sniff.SOC_TestConfig_3" />
			<class name="org.exoplatform.selenium.platform.social.sniff.SOC_Notifications_IntranetNotifications_NotificationTypes" />
		    <class name="org.exoplatform.selenium.platform.social.sniff.SOC_Notifications_UserSettings" />
		    <class name="org.exoplatform.selenium.platform.social.sniff.SOC_Notifications_Administration" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>