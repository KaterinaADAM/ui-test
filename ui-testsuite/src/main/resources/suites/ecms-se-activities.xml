<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ECMS Functional Test" verbose="1" preserve-order="true">
	<test name="ecms-func-se-activities" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_ContentActivities_Action" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_ContentActivities_Add" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_ContentActivities_Delete" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_ContentActivities_NotUpdateAS" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_ContentActivities_Update" />
			<class
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.publishactivities.ECMS_SE_PublishActivities_FilesActivities" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>