<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="wiki-sniff" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.social.sniff.SOC_TestConfig" />
			<class name="org.exoplatform.selenium.platform.social.sniff.SOC_Member_Management" />
			<class name="org.exoplatform.selenium.platform.social.sniff.SOC_Space_Management" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>