<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="plf-sniff-navigation" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Integration"/>
            <class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Registration"/>
            <class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Navigation_UserNavigation"/>
            <class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Navigation_TopNavigation"/>
            <class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Navigation_SpaceNavigation"/>
            <class name="org.exoplatform.selenium.platform.plf.sniff.PLF_Navigation_LeftNavigation"/>
        </classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>